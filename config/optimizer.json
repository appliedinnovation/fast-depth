{
  "algorithm": "bayes",

  "parameters": {
    "batch_size": {
      "type": "discrete",
      "values": [128, 256]
    },
    "learning_rate" : {
        "type" : "float",
        "min" : 0.001,
        "max" : 0.1
    },
    "momentum" : {
      "type" : "float",
      "min" : 0.7,
      "max" : 0.95
    },
    "weight_decay" : {
      "type" : "float",
      "min" : 0.00001,
      "max" : 0.001
    }
  },

  "spec": {
    "metric": "loss",
    "objective": "minimize"
  }
}
